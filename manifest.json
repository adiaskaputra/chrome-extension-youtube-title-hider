{
  "manifest_version": 3,
  "name": "YouTube Title Hider",
  "version": "1.0",
  "description": "Menyembunyikan judul video di YouTube",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}
